<h1>Your friend list</h1>
<%= link_to "Incoming Message", incoming_messages_path %>
<%= link_to "Outgoing Message", outgoing_messages_path %>
<%= link_to "Add Friend", users_path %>
<%= link_to "New Message", new_message_path %>
<% if @friendlist%>
	<table>
	<% @friendlist.each do |friend| %>
	<tr>
			<td><%= friend.email %></td>
	</tr>
	<% end %>
	</table>
<% else %>
	<p>You haven't added any friend yet. Let make some now: </p>
<% end %>

